@model List<NqtProduct>

<div class="row">
    @foreach (var product in Model)
    {
        <div class="col-md-4">
            <div class="product-item">
                <img src="@product.ImageUrl" alt="@product.Name" />
                <h5>@product.Name</h5>
            </div>
        </div>
    }
</div>